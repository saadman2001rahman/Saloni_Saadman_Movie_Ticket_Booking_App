<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="498.0" prefWidth="513.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.SearchMovieController">
   <children>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label text="Search by Movie name" />
            <TextField fx:id="search_movie_name" />
            <Button fx:id="search_movie_name_button" mnemonicParsing="false" onAction="#search_movie_by_name" prefHeight="28.0" prefWidth="59.0" text="Search" />
         </children>
      </HBox>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label text="Seach by genre" />
            <TextField fx:id="search_movie_genre" />
            <Button fx:id="search_movie_genre_button" mnemonicParsing="false" onAction="#search_movie_by_genre" text="Search" />
         </children>
      </HBox>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label text="Movie info" />
         </children>
      </HBox>
	  <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label text="Movie 3" />
         </children>
      </HBox>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label text="Movie 4" />
         </children>
      </HBox>
      
   </children>
</VBox>
